{
  "name": "@ima/cli-plugin-scramble-css",
  "version": "1.0.0",
  "description": "Plugin for @ima/cli that implements CSS class minimizer and uglifier that can be reverse-compiled at runtime.",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./scrambleCssClasses": {
      "types": "./dist/scrambleCssClasses.d.ts",
      "default": "./dist/scrambleCssClasses.js"
    }
  },
  "scripts": {
    "build": "ima-plugin build --nodeConfig",
    "dev": "ima-plugin dev --nodeConfig",
    "link": "ima-plugin link --nodeConfig",
    "lint": "eslint './**/*.{js,jsx,ts,tsx,mjs}'"
  },
  "keywords": [
    "IMA",
    "cli",
    "plugin",
    "scramble",
    "css",
    "minimize"
  ],
  "author": "Jan Šimeček <jan.simecek@firma.seznam.cz>",
  "repository": {
    "type": "git",
    "url": "https://github.com/seznam/IMA.js-plugins.git",
    "directory": "packages/cli-plugin-scramble-css"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/seznam/IMA.js-plugins/issues"
  },
  "license": "MIT",
  "peerDependencies": {
    "@ima/cli": ">=18.0.0",
    "@ima/react-page-renderer": ">=18.0.0",
    "classnames": "*",
    "copy-webpack-plugin": "*",
    "postcss": ">=8",
    "webpack": ">=5"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/react": "^18.0.17"
  },
  "dependencies": {
    "postcss-selector-parser": "^6.0.10",
    "schema-utils": "^4.0.0"
  }
}