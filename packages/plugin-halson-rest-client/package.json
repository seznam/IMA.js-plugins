{
  "name": "@ima/plugin-halson-rest-client",
  "version": "1.1.0",
  "description": "HAL+JSON REST API client for IMA applications.",
  "main": "dist/main",
  "module": "dist/main",
  "exports": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.cjs"
  },
  "browser": {
    "./dist/main.js": "./dist/main.es5.js",
    "./dist/main.cjs": "./dist/main.es5.js",
    "./dist/main.mjs": "./dist/main.mjs"
  },
  "scripts": {
    "build": "echo \"Release process is supported only via lerna command. See README.md for more info.\"; exit 1",
    "test": "../../node_modules/.bin/jest --config=jest.config.js",
    "test:es:version": "../../node_modules/.bin/es-check es5 ./dist/main.es5.js && ../../node_modules/.bin/es-check --module es9 ./dist/main.mjs",
    "doc": "../../node_modules/.bin/gulp doc",
    "prepare": "../../node_modules/.bin/rollup -c"
  },
  "keywords": [
    "ima",
    "ima.js",
    "rest",
    "hal",
    "json",
    "halson",
    "plugin"
  ],
  "author": "Martin Jurƒça <martin.jurca@firma.seznam.cz>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/seznam/IMA.js-plugins.git"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/",
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/seznam/IMA.js-plugins/tree/master/packages/plugin-halson-rest-client/issues"
  },
  "homepage": "https://github.com/seznam/IMA.js-plugins/tree/master/packages/plugin-halson-rest-client#readme",
  "dependencies": {
    "@ima/plugin-rest-client": "^3.1.0",
    "halson": "3.0.0",
    "uri-template": "1.0.1"
  },
  "peerDependencies": {
    "@ima/core": ">= 17.0.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
