{
  "name": "babel-plugin-ima-logger",
  "version": "4.0.0",
  "description": "The babel pluging that removes @ima/plugin-logger from source code at compilation time.",
  "main": "./dist/main.js",
  "scripts": {
    "test": "../../node_modules/.bin/jest --coverage --no-watchman --config=jest.config.js",
    "build": "ima-plugin build --nodeConfig",
    "dev": "ima-plugin dev --nodeConfig",
    "link": "ima-plugin link --nodeConfig",
    "lint": "eslint './**/*.{js,jsx,ts,tsx,mjs}'"
  },
  "keywords": [
    "IMA.js",
    "Logger",
    "Plugin",
    "Javascript"
  ],
  "author": "Ondrej Base <ondrej.base@firma.seznam.cz>",
  "repository": {
    "type": "git",
    "url": "https://github.com/seznam/IMA.js-plugins.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/seznam/IMA.js-plugins/issues"
  },
  "license": "MIT",
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
