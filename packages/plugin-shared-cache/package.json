{
  "name": "@ima/plugin-shared-cache",
  "version": "2.1.0",
  "description": "A utility for creating caches that are shared between requests by all IMA.js application instances within the same node.js process.",
  "main": "dist/main",
  "module": "dist/main",
  "exports": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.cjs"
  },
  "browser": {
    "./dist/main.js": "./dist/main.es5.js",
    "./dist/main.cjs": "./dist/main.es5.js",
    "./dist/main.mjs": "./dist/main.mjs"
  },
  "scripts": {
    "build": "echo \"Release process is supported only via lerna command. See README.md for more info.\"; exit 1",
    "test": "../../node_modules/.bin/jest --coverage --no-watchman --config=jest.config.js",
    "test:es:version": "../../node_modules/.bin/es-check es5 ./dist/main.es5.js && ../../node_modules/.bin/es-check --module es9 ./dist/main.mjs",
    "doc": "../../node_modules/.bin/gulp doc",
    "prepare": "../../node_modules/.bin/rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seznam/IMA.js-plugins.git"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "ima",
    "ima.js",
    "plugin",
    "cache",
    "shared"
  ],
  "author": "Martin Jurƒça <martin.jurca@firma.seznam.cz>",
  "license": "UNLICENSED",
  "dependencies": {
    "clone": "^2.1.2"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=4 <8"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
