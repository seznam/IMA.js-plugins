{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "env": [
        "STATIC_PATH",
        "API_BASE_URL",
        "NODE_CONFIG_ENV",
        "NODE_ENV"
      ],
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {},
    "test": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
